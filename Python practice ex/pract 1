first_num = 9
second_num = 7.8
my_str = "start"
print(first_num), print(second_num), print(my_str)

first_num = 5.2
print(first_num)
print(type(first_num))
third_num = (first_num + second_num)
print(third_num)
print(type(third_num))

first_num += 5
second_num += first_num
print(first_num)
print(second_num)
second_num = int(second_num)
print(second_num)
my_str += "&stop"
print(my_str)
my_str = my_str * 5
print(my_str)
