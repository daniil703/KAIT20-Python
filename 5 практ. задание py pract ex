#4 Задание
mass = [14, -6, 3, 11, 6, 8.5, 99, -20, -6, 10, 40, 0.25, -4.5]
mass.remove(-6)
mass.remove(-20)
mass.remove(-6)
mass.remove(-4.5)
mass.sort()
print(mass)
mass.sort(reverse=True)
print(mass)
